{
    "title": "Flask App Metrics",
    "panels": [
      {
        "title": "Request Rate",
        "type": "graph",
        "targets": [{
          "expr": "rate(flask_app_request_count_total[1m])",
          "legendFormat": "{{method}} {{endpoint}}"
        }]
      },
      {
        "title": "Error Rates",
        "type": "piechart",
        "targets": [{
          "expr": "sum by (error_type) (flask_app_error_count_total)",
          "legendFormat": "Error {{error_type}}"
        }]
      },
      {
        "title": "Prediction Latency",
        "type": "heatmap",
        "targets": [{
          "expr": "model_prediction_latency_seconds_bucket",
          "legendFormat": "Latency distribution"
        }]
      }
    ]
  }